[module/previous]
type = custom/script
inherit = section/base_right_dark
interval = 86400
format = "%{T7}<label>%{T-}"
format-padding = 2
; Previous song icon
exec = echo "Û∞íÆ"
; Check if spotify is running before displaying the icon
; exec-if = "pgrep spotify"
line-size = 1
click-left = "dbus-send --print-reply --dest=org.mpris.MediaPlayer2.spotify /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.Previous"

[module/next]
type = custom/script
inherit = section/base_right_dark
interval = 86400
format = "%{T7}<label>"
format-padding = 2
; Next song icon
exec = echo "Û∞í≠"
; Check if spotify is running before displaying the icon
; exec-if = "pgrep spotify"
line-size = 1
click-left = "dbus-send --print-reply --dest=org.mpris.MediaPlayer2.spotify /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.Next"

[module/playpause]
type = custom/ipc
inherit = section/base_right_dark
format = %{T5}<output>%{T-}
; Default
hook-0 = echo "Û∞êé"
; Playing
hook-1 = echo "Û∞è§"
; Paused
hook-2 = echo "Û∞êä"
initial = 1
click-left = "dbus-send --print-reply --dest=org.mpris.MediaPlayer2.spotify /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.PlayPause"

[module/spotify]
type = custom/ipc
inherit = section/base_right_dark
hook-0 = echo "Not playing"
hook-1 = python ~/.config/polybar/spotify_status.py
initial = 1
format-padding = 4
click-left = i3-msg '[class="Spotify"] focus'

[module/backlight]
type = custom/ipc
hook-0 = xbacklight -get
inherit = section/base_right_dark
format-prefix = %{T7}Û∞Éü %{T-}
format = <output>%
initial = 1

[module/filesystem]
type = internal/fs
interval = 25
mount-0 = /
label-mounted = %{T6}Û∞ãä %{T-}%percentage_used%%
label-mounted-background = ${colors.material-dark-background}
label-mounted-padding = ${variables.label-padding}

[module/user]
type = custom/script
exec = getent passwd | grep $USER | awk -F ':' '{print $5}'
format-prefix = %{T7}üë® %{T-}
format = <label>
format-background = ${colors.material-primary}
format-padding = ${variables.label-padding}

[module/eth]
type = internal/network
interface = docker0
interval = 3.0

format-connected-underline = #55aa55
format-connected-prefix = "Ôûñ "
format-connected-prefix-foreground = ${colors.foreground-alt}
label-connected = %local_ip%

format-disconnected =
;format-disconnected = <label-disconnected>
;format-disconnected-underline = ${self.format-connected-underline}
;label-disconnected = %ifname% disconnected
;label-disconnected-foreground = ${colors.foreground-alt}

